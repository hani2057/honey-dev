{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@apis/(.*)$",
    "^@store/(.*)$",
    "^@types/(.*)$",
    "^@pages/(.*)$",
    "^@components/(.*)$",
    "@/(.*)",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
